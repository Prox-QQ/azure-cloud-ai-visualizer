# OpenAI Configuration (for testing)
USE_OPENAI_FALLBACK=true
OPENAI_API_KEY=sk-proj
AZURE_OPENAI_KEY=sk-proj
OPENAI_MODEL=gpt-4o
AZURE_MCP_BICEP_URL=https://learn.microsoft.com/api/mcp/tools/azure-bicep-schema
TERRAFORM_MCP_URL=https://developer.hashicorp.com/terraform/mcp-server
AZURE_MCP_BICEP_FORCE=true
TERRAFORM_MCP_FORCE=true

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
CORS_ORIGINS=["http://localhost:3000", "http://localhost:5173", "http://localhost:8080"]
DEV_ANALYZE_BYPASS=false

# Azure Configuration (for production - currently not used)
AZURE_SUBSCRIPTION_ID=placeholder
AZURE_TENANT_ID=placeholder
AZURE_RESOURCE_GROUP=placeholder
AZURE_STORAGE_ACCOUNT_NAME=placeholder
AZURE_AI_PROJECT_ENDPOINT=placeholder
AZURE_AI_MODEL_DEPLOYMENT_NAME=gpt-4o

# Storage Containers
AZURE_STORAGE_CONTAINER_NAME_PROJECTS=projects
AZURE_STORAGE_CONTAINER_NAME_ASSETS=assets
AZURE_STORAGE_CONTAINER_NAME_EXPORTS=exports
AZURE_STORAGE_CONTAINER_NAME_IAC=iac
AZURE_STORAGE_CONTAINER_NAME_DEPLOYMENTS=deployments
AZURE_STORAGE_CONTAINER_NAME_CONVERSATIONS=conversations


# Azure AI Project (for MAF)
AZURE_AI_PROJECT_ENDPOINT=https://your-project.cognitiveservices.azure.com/
AZURE_AI_MODEL_DEPLOYMENT_NAME=gpt-4o

# Azure OpenAI (alternative to AI Project)
AZURE_OPENAI_ENDPOINT=https://your-openai.openai.azure.com/
AZURE_OPENAI_API_KEY=your-openai-key
AZURE_OPENAI_API_VERSION=2024-06-01

# Azure Key Vault
AZURE_KEY_VAULT_URL=https://your-keyvault.vault.azure.net/


# Chat and WebSocket
CHAT_MAX_HISTORY=50
WEBSOCKET_PING_INTERVAL=30

# IaC Generation
DEFAULT_AZURE_REGION=westeurope
BICEP_OUTPUT_FORMAT=json
TERRAFORM_VERSION=1.5.0

# Deployment
DEPLOYMENT_TIMEOUT_MINUTES=30
MAX_CONCURRENT_DEPLOYMENTS=3